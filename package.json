    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/history": "^4.7.2",
    "@types/moment-timezone": "^0.5.10",
